                                <option value="Value" selected>Select User Type</option>

                                <option value="Value">
                                    Text
                                </option>
                            </select>
                        </div>
                        <div class="input-field-common">
                            <label class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                User
                            </label>
                            <input type="text" class="w-full" placeholder="User Name" name="Username"
                                   maxlength="11">
                        </div>

                        <div class="input-field-common">
                            <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                Transfer Type
                            </label>
                            <select id="large" name="Status" class=" default_select select-common block w-full "
                                    style="width: 100%;">

                                <option value="Value" selected>All</option>

                                <option value="Value">
                                    Text
                                </option>
                            </select>
                        </div>

                    </div>

                    @*"filter Point Request"*@

                    <div id="Point_Requests1" class="pane" style="display: none;">

                        <div class=" grid grid-cols-3 gap-[27px]">
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    Location
                                </label>
                                <select id="large" name="Status" class=" default_select select-common block w-full "
                                        style="width: 100%;">

                                    <option value="Value" selected>Select User Type</option>

                                    <option value="Value">
                                        Text
                                    </option>
                                </select>
                            </div>
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                    Club Name
                                </label>
                                <input type="text" class="w-full" placeholder="User Name" name="Username"
