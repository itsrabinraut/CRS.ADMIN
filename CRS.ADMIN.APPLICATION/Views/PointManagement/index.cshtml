
@{
    ViewBag.Title = "PointManagement";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="px-8 py-4">
    <div>
        <div class="heading-title">
            Points Management
        </div>
        <div class="span-title-light-gray">
            Manage your points details.
        </div>
    </div>

    <div class="flex gap-4 my-8 border-b ">
        <div id="tab1Link" class="text-[#667085] text-[14px] styleCss active px-1 pb-4 cursor-pointer active" onclick="changeTab('Points_Transfer', 'tab1Link')">
            Points Transfer
        </div>
        <div id="tab2Link" class="text-[#667085] text-[14px] styleCss px-1 pb-4 cursor-pointer" onclick="changeTab('Points_Balance', 'tab2Link')">
            Points Balance Statement
        </div>

        <div id="tab3Link" class="text-[#667085] text-[14px] styleCss px-1 pb-4 cursor-pointer" onclick="changeTab('System_Transfer', 'tab3Link')">
            System Transfer
        </div>
        <div id="tab4Link" class="text-[#667085] text-[14px] styleCss px-1 pb-4 cursor-pointer" onclick="changeTab('Point_Requests', 'tab4Link')">
            Point Requests
        </div>
    </div>


    <div class="flex justify-between pb-[24px]">



        <div class="relative input-search-wrapper">
            <div class="absolute inset-y-0 left-0 flex items-center pl-[16px] pointer-events-none">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="21" viewBox="0 0 20 21" fill="none">
                    <path d="M17.5 18L13.875 14.375M15.8333 9.66667C15.8333 13.3486 12.8486 16.3333 9.16667 16.3333C5.48477 16.3333 2.5 13.3486 2.5 9.66667C2.5 5.98477 5.48477 3 9.16667 3C12.8486 3 15.8333 5.98477 15.8333 9.66667Z"
                          stroke="#667085" stroke-width="1.66667" stroke-linecap="round" stroke-linejoin="round" />
                </svg>
            </div>
            <input type="search"
                   id="default-search"
                   class="input-search block w-full "
                   placeholder="Search"
                   value="SearchFilter"
                   name="SearchFilter">
        </div>

        <div class="flex gap-6">
            <div class="export-button h-[40px]">
                <img src="~/Content/assets_new/images/club/download-solid.svg">Export
            </div>

            <div class="btn btn-primary gap-[8px] h-[40px]" data-drawer-target="add-new-host-club"
                 data-drawer-show="add-new-host-club" data-drawer-placement="right" data-drawer-backdrop="false"
                 aria-controls="drawer-disabled-backdrop" id="add-new-id">
                <img src="~/Content/assets_new/images/plus-circle-solid.svg" />
                Add_New
            </div>
        </div>
    </div>

