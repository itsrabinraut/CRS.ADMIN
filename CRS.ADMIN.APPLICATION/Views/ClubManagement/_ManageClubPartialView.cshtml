@model CRS.ADMIN.APPLICATION.Models.ClubManagement.ManageClubModel
@{
//ViewBag.Title = "Club Management";
//Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="add-new-host-club" class="drawer-close-disable  flex justify-end fixed top-0 right-0 z-40 h-screen overflow-y-auto transition-transform translate-x-full "
     tabindex="-1" aria-labelledby="drawer-right-label">
    <div class="bg-white w-[863px] drawer-container relative">
        <div class="drawer-form-container sticky top-0 z-[5] ">
                   <div class="drawer-form flex justify-between bg-secondary-color-pink py-[16px] px-[24px] h-[56px]">
                       <div>  @CRS.ADMIN.APPLICATION.Resources.Resource.Manage_Club_Profile</div>
                <button type="button" data-drawer-hide="add-new-host-club" aria-controls="add-new-host-club"
                        class="text-[20px]">
                    <i class="las la-window-close text-white"></i>
                </button>
            </div>
        </div>
        <div class="p-[24px]">

            <div class=" py-4 grid gap-6">


                <div class="flex gap-2 ">
                    <div class="flex items-center gap-2 profile-steps add-new-host-wizard  active" id="step1">
                        <div class="steps-number ">
                            1
                        </div>
                        <div class="steps-text">
                            @CRS.ADMIN.APPLICATION.Resources.Resource.Basic_Information
                        </div>
                        <hr class="setp-divider  flex-1" style="width: 121px;" />

                    </div>
                    <div class="flex items-center gap-2 profile-steps add-new-host-wizard " id="step2">
                        <div class="steps-number ">
                            2
                        </div>
                        <div class="steps-text">
                            @CRS.ADMIN.APPLICATION.Resources.Resource.Social_Links
                        </div>
                        <hr class="setp-divider" style="width: 121px;" />

                    </div>
                    <div class="flex items-center gap-2 profile-steps add-new-host-wizard " id="step3">
                        <div class="steps-number ">
                            3
                        </div>
                        <div class="steps-text">
                            @CRS.ADMIN.APPLICATION.Resources.Resource.Club_Info  
                        </div>
                        <hr class="setp-divider" style="width: 121px;" />

                    </div>
                    <div class="flex items-center gap-2 profile-steps add-new-host-wizard " id="step3">
                        <div class="steps-number ">
                            4
                        </div>
                        <div class="steps-text">
                            @CRS.ADMIN.APPLICATION.Resources.Resource.Plans
                        </div>
                        <!-- <hr class="setp-divider" style="width: 121px;" /> -->

                    </div>

                </div>

                <!---------------------------------------------------- Start Basic Info ---------------------------------------------------------------->
                <div id="content1" class="content-section " style="display: block;">
                    <div class="grid gap-4 py-[10px]">
                        <div class="flex gap-2 text-primary-300 text-sm font-semibold items-center mt-[10px]">
                            <div class="flex-none uppercase">@CRS.ADMIN.APPLICATION.Resources.Resource.Basic_Info</div>
                            <div class="bg-[#F4F4F4] h-[1px] w-full">
                            </div>
                        </div>
                        <div class="grid grid-cols-3 gap-x-5 gap-y-4">
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_English
                                </label>
                                @Html.TextBoxFor(model => model.ClubName1, new
                           {
                               @class = "w-full",
                               @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_English_Placeholder,
                               @onkeypress = "return isAlphabetWithSpace(event)",
                               autocomplete = "off",
                               @maxLength = "50"
                           })
                                @* <input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_English_Placeholder" name="ClubName1" value="@Model.ClubName1">*@
                                <span class="error">
                                    @Html.ValidationMessageFor(m => m.ClubName1)
                                </span>
                            </div>
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_Katakana
                                </label>
                                @Html.TextBoxFor(model => model.ClubName2, new
                           {
                               @class = "w-full",
                               @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_Katakana_Placeholder,
                               @onkeypress = "return isAlphabetWithSpace(event)",
                               autocomplete = "off",
                               @maxLength = "50"
                           })
                                @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Club_Name_Katakana_Placeholder" name="ClubName2" value="@Model.ClubName2">*@
                                <span class="error">
                                    @Html.ValidationMessageFor(m => m.ClubName2)
                                </span>
                            </div>
                        </div>
                        <div class="grid grid-cols-3 gap-x-5 gap-y-4">
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Group_Name
                                </label>
                                @Html.TextBoxFor(model => model.GroupName, new
                           {
                               @class = "w-full",
                               @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Group_Name_Placeholder,
                               //@onkeypress = "return isAlphabetWithSpace(event)",
                               autocomplete = "off",
                               @maxLength = "50"
                           })
                                @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Group_Name_Placeholder" name="GroupName" value="@Model.GroupName">*@
                                <span class="error">
                                    @Html.ValidationMessageFor(m => m.GroupName)
                                </span>
                            </div>
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Business_Type
                                </label>
                                <select id="large" name="BusinessTypeDDL" class=" default_select select-common block w-full "
                                        style="width: text-2%;">
                                    @foreach (var item in ViewBag.BusinessTypeDDL as IEnumerable<SelectListItem>)
                                    {
                                        var selectedId = ViewBag.BusinessTypeKey as string;
                                        if (item.Value == selectedId)
                                        {
                                            <option value="@item.Value" selected>
                                                @item.Text
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="@item.Value">
                                                @item.Text
                                            </option>
                                        }
                                    }
                                </select>
                            </div>
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Company_Name
                                </label>
                                @Html.TextBoxFor(model => model.CompanyName, new
                           {
                               @class = "w-full",
                               @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Company_Name_Placeholder,
                               //@onkeypress = "return isAlphabetWithSpace(event)",
                               autocomplete = "off",
                               @maxLength = "50"
                           })
                                @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Company_Name_Placeholder" name="CompanyName" value="@Model.CompanyName">*@
                                <span class="error">
                                    @Html.ValidationMessageFor(m => m.CompanyName)
                                </span>
                            </div>
                        </div>
                        <!-- for email contact number and landline -->

                        <div class="grid grid-cols-3 gap-x-5 gap-y-4">
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Email
                                </label>
                                @if (!string.IsNullOrEmpty(Model.AgentId))
                                {
                                    <input type="text" class="w-full" placeholder="e.g abc@gmail.om" name="@CRS.ADMIN.APPLICATION.Resources.Resource.Email" value="@Model.Email" disabled>
                                }
                                else
                                {
                                    @Html.TextBoxFor(model => model.Email, new
                               {
                                   @class = "w-full",
                                   @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Email,
                                   //@onkeypress = "return isAlphabetWithSpace(event)",
                                   autocomplete = "off",
                                   @maxLength = "200"
                               })
                                    @*<input type="text" class="w-full" placeholder="e.g abc@gmail.om" name="@CRS.ADMIN.APPLICATION.Resources.Resource.Email" value="@Model.Email">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.Email)
                                    </span>
                                }
                            </div>
                            <div class="input-field-common w-full">
                                <label class="block mb-4 text-sm font-medium text-secondary-100 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Phone_Number
                                </label>
                                <div class="number-input w-full">
                                    <div class="flag-section">
                                        <div class="japan-flag">
                                            <div></div>
                                        </div>
                                        <div>
                                            +81
                                        </div>
                                        <div class="line-y">

                                        </div>

                                    </div>
                                    @if (!string.IsNullOrEmpty(Model.AgentId))
                                    {
                                        <input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Phone_Number" name="MobileNumber" value="@Model.MobileNumber" disabled>
                                    }
                                    else
                                    {
                                    @Html.TextBoxFor(model => model.MobileNumber, new
                               {
                                   @class = "w-full",
                                   @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Phone_Number,
                                   @onkeypress = "return isNumber(event)",
                                   autocomplete = "off",
                                   @maxLength = "11"
                               })
                                        @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Phone_Number" name="MobileNumber" value="@Model.MobileNumber">*@
                                        <span class="error">
                                            @Html.ValidationMessageFor(m => m.MobileNumber)
                                        </span>
                                    }
                                    @*<input type="text" class="w-full" placeholder="090-6490-3254" name="mobileNumber" required>*@

                                </div>
                            </div>
                            <!-- for landline -->
                            <div class="input-field-common w-full">
                                <label class="block mb-4 text-sm font-medium text-secondary-100 ">
                                    Landline Number
                                </label>
                                <div class="number-input w-full">
                                    <div class="flag-section">
                                        <div class="japan-flag">
                                            <div></div>
                                        </div>
                                        <div>
                                            +81
                                        </div>
                                        <div class="line-y">

                                        </div>

                                    </div>
                                    @Html.TextBoxFor(model => model.LandlineNumber, new
                               {
                                   @class = "w-full",
                                   @placeholder = "090-6490-3254",
                                   @onkeypress = "return isNumber(event)",
                                   autocomplete = "off",
                                   @maxLength = "11"
                               })
                                    @*<input type="text" class="w-full" placeholder="090-6490-3254" name="landLineNumber" required>*@

                                </div>
                            </div>
                        </div>
                        <!-- email info end -->
                        <!-- description start -->
                        <div class="input-field-common">
                            <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                @CRS.ADMIN.APPLICATION.Resources.Resource.Description
                            </label>
                            @Html.TextAreaFor(model => model.Description, new
                       {
                           @class = "w-full",
                           @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Description_Placeholder,
                           //@onkeypress = "return isNumber(event)",
                           autocomplete = "off",
                           @maxLength = "500"
                       })
                            
                            <span class="error">
                                @Html.ValidationMessageFor(m => m.Description)
                            </span>
                        </div>
                        <!-- description end -->
                        <!-- for user Information -->
                        <div class="flex gap-2 text-primary-300 text-sm font-semibold items-center mt-[10px]">
                            <div class="flex-none uppercase">  @CRS.ADMIN.APPLICATION.Resources.Resource.User_Info</div>
                            <div class="bg-[#F4F4F4] h-[1px] w-full">
                            </div>
                        </div>

                        <!-- name and username start -->
                        <div class="grid grid-cols-3 gap-x-5 gap-y-4">
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    CEO Full Name
                                </label>
                                @Html.TextBoxFor(model => model.ceoFullName, new
                           {
                               @class = "w-full",
                               @placeholder = "CEO",
                               //@onkeypress = "return isNumber(event)",
                               autocomplete = "off"
                               //@maxLength = "30"
                           })
                                @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Username" name="LoginId" value="@Model.LoginId">*@
                                <span class="error">
                                    @Html.ValidationMessageFor(m => m.ceoFullName)
                                </span>
                                @*<input type="text" class="w-full" placeholder="CEO Full Name" name="ceoFullName" required>*@
                            </div>
                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Username
                                </label>
                                @if (!string.IsNullOrEmpty(Model.AgentId))
                                {
                                    <input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Username" name="LoginId" value="@Model.LoginId" disabled>
                                }
                                else
                                {
                                    @Html.TextBoxFor(model => model.LoginId, new
                               {
                                   @class = "w-full",
                                   @placeholder = @CRS.ADMIN.APPLICATION.Resources.Resource.Username,
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                    @*<input type="text" class="w-full" placeholder="@CRS.ADMIN.APPLICATION.Resources.Resource.Username" name="LoginId" value="@Model.LoginId">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.LoginId)
                                    </span>
                                }
                            </div>
                        </div>
                        <!-- name username section end -->
                        <!-- logo and cover page start -->
                        <div class="grid grid-cols-2 gap-x-5 gap-y-4">
                            <div>
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Logo
                                </label>
                                <div class="file-upload-common">
                                    <label for="file_input_logo" class="custom-file-upload bg-[#CC527A] w-[123px] flex-none ">
                                        <span> @CRS.ADMIN.APPLICATION.Resources.Resource.Choose_Images</span>
                                    </label>
                                    <input id="file_input_logo" class="file-input" name="logo",value="@Model.Logo" type="file" style="display:none;">
                                    <p id="file_name_logo"
                                       class="file-name w-max h-[23px] overflow-hidden pl-2 text-xs text-[#686868] leading-[25px]">
                                        @if (string.IsNullOrEmpty(Model.Logo))
                                        {
                                            @CRS.ADMIN.APPLICATION.Resources.Resource.No_File_Choosen
                                        }
                                        else
                                        {
                                            string fileName = Path.GetFileName(Model.Logo);
                                            @fileName
                                        }
                                   </div>
                                <div class="text-xs text-error mt-[8px]">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Note:
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Image_should_be_in_HEIF_format
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.and_Maximum_file_size_is_5_MB .
                                </div>
                            </div>

                            <div>
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Cover_Photo
                                </label>
                                <div class="file-upload-common">
                                    <label for="file_input_cover" class="custom-file-upload bg-[#CC527A] w-[123px] flex-none ">
                                        <span>  @CRS.ADMIN.APPLICATION.Resources.Resource.Choose_Images</span>
                                    </label>
                                    <input id="file_input_cover" class="file-input" name="coverImage" type="file" value="@Model.CoverPhoto"  style="display:none;">
                                    <p id="file_name_logo"
                                       class="file-name w-max h-[23px] overflow-hidden pl-2 text-xs text-[#686868] leading-[25px]">
                                        @if (string.IsNullOrEmpty(Model.CoverPhoto))
                                        {
                                            @CRS.ADMIN.APPLICATION.Resources.Resource.No_File_Choosen
                                        }
                                        else
                                        {
                                            string fileName = Path.GetFileName(Model.CoverPhoto);
                                            @fileName
                                        }
                             </div>
                                <div class="text-xs text-error mt-[8px]">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Note:
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Image_should_be_in_HEIF_format
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.and_Maximum_file_size_is_5_MB .
                                </div>
                            </div>

                        </div>
                        <!-- logo and cover page end -->
                        <div>
                            <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                @CRS.ADMIN.APPLICATION.Resources.Resource.Business_Cerfificate
                            </label>
                            <div class="file-upload-common">
                                <label for="file_input_business_certificate" class="custom-file-upload w-[123px] bg-[#CC527A] flex-none ">
                                    <span>@CRS.ADMIN.APPLICATION.Resources.Resource.Choose_Images</span>
                                </label>
                                <input id="file_input_business_certificate" class="file-input" type="file" name="businessCertificate" value="@Model.BusinessCertificate"
                                       style="display:none;">
                                <p id="file_name_bussiness_certificate"
                                   class="file-name w-max h-[23px] overflow-hidden pl-2 text-xs text-[#686868] leading-[25px]">
                                    @if (string.IsNullOrEmpty(Model.BusinessCertificate))
                                    {
                                        @CRS.ADMIN.APPLICATION.Resources.Resource.No_File_Choosen
                                    }
                                    else
                                    {
                                        string fileName = Path.GetFileName(Model.BusinessCertificate);
                                        @fileName
                                    }
                               </div>
                            <div class="text-xs text-error mt-[8px]">
                                @CRS.ADMIN.APPLICATION.Resources.Resource.Note:
                                @CRS.ADMIN.APPLICATION.Resources.Resource.Image_should_be_in_HEIF_format
                                @CRS.ADMIN.APPLICATION.Resources.Resource.and_Maximum_file_size_is_5_MB .
                            </div>
                        </div>
                    </div>

                </div>

                <!---------------------------------------------------- END Basic Info ---------------------------------------------------------------->
                <!---------------------------------------------------- Start social link---------------------------------------------------------------->

                <div id="content2" class="content-section" style="display: none;">
                    <div class="grid gap-4 py-[10px]">
                        <div class="flex gap-2 text-primary-300 text-sm font-semibold items-center mt-[10px]">
                            <div class="flex-none uppercase"> @CRS.ADMIN.APPLICATION.Resources.Resource.Social_Links</div>
                            <div class="bg-[#F4F4F4] h-[1px] w-full">
                            </div>
                        </div>
                        <div class="grid  gap-4">
                            <div class="grid grid-cols-2 gap-x-4 gap-y-5">
                                <div class="input-field-common">
                                    <label for="instagram" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        @CRS.ADMIN.APPLICATION.Resources.Resource.Instagram
                                    </label>

                                    @Html.TextBoxFor(model => model.InstagramLink, new
                               {
                                   @class = "w-full",
                                   @placeholder = @CRS.ADMIN.APPLICATION.Resources.Resource.Instagram,
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                    @*<input type="text" class="w-full" placeholder=" @CRS.ADMIN.APPLICATION.Resources.Resource.Instagram" name="InstagramLink" value="@Model.InstagramLink"
                                    ="">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.InstagramLink)
                                    </span>
                                </div>
                                <div class="input-field-common">
                                    <label for="tiktok" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        @CRS.ADMIN.APPLICATION.Resources.Resource.Tiktok
                                    </label>
                                    @Html.TextBoxFor(model => model.TiktokLink, new
                               {
                                   @class = "w-full",
                                   @placeholder = @CRS.ADMIN.APPLICATION.Resources.Resource.Tiktok,
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                    @*<input type="text" class="w-full" placeholder=" @CRS.ADMIN.APPLICATION.Resources.Resource.Tiktok" name="TiktokLink" value="@Model.TiktokLink"
                                     ="">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.TiktokLink)
                                    </span>
                                </div>
                            </div>

                            <div class="grid grid-cols-2 gap-x-4 gap-y-5">
                                <div class="input-field-common">
                                    <label for="line" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        Line
                                    </label>
                                    @Html.TextBoxFor(model => model.Line, new
                               {
                                   @class = "w-full",
                                   @placeholder = "i.e. www.line.com/+09033333333 ",
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                  
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.Line)
                                    </span>
                                    @*<input type="text" class="w-full" placeholder="i.e. www.line.com/+09033333333 " name="line" required>
                                   <span class="error">Validation msg</span>*@
                                </div>
                                <div class="input-field-common">
                                    <label for="twitter" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        @CRS.ADMIN.APPLICATION.Resources.Resource.Twitter
                                    </label>
                                    @Html.TextBoxFor(model => model.TwitterLink, new
                               {
                                   @class = "w-full",
                                   @placeholder = @CRS.ADMIN.APPLICATION.Resources.Resource.Twitter,
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                    @*<input type="text" class="w-full" placeholder=" @CRS.ADMIN.APPLICATION.Resources.Resource.Tiktok" name="TiktokLink" value="@Model.TiktokLink"
                                     ="">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.TwitterLink)
                                    </span>
                                </div>
                            </div>

                            <!-- for other links -->
                            <div class="flex gap-2 text-primary-300 text-sm font-semibold items-center mt-[10px]">
                                <div class="flex-none uppercase">Other Links</div>
                                <div class="bg-[#F4F4F4] h-[1px] w-full">
                                </div>
                            </div>
                            <div class="grid grid-cols-2 gap-x-4 gap-y-5">
                                <div class="input-field-common">
                                    <label for="website" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        @CRS.ADMIN.APPLICATION.Resources.Resource.Website
                                    </label>

                                    @Html.TextBoxFor(model => model.WebsiteLink, new
                               {
                                   @class = "w-full",
                                   @placeholder = "i.e. www.clubglobal.com",
                                   //@onkeypress = "return isNumber(event)",
                                   autocomplete = "off"
                                   //@maxLength = "30"
                               })
                                    @*<input type="text" class="w-full" placeholder="i.e. www.clubglobal.com" name="WebsiteLink" value="@Model.WebsiteLink" ="">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.WebsiteLink)
                                    </span>
                                </div>
                                <div class="input-field-common">
                                    <label for="googleMap" class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                        Google Map
                                    </label>
                                     @Html.TextBoxFor(model => model.GoogleMap, new
                                {
                                    @class = "w-full",
                                    @placeholder = "i.e. www.clubglobal.com",
                                    //@onkeypress = "return isNumber(event)",
                                    autocomplete = "off"
                                    //@maxLength = "30"
                                })
                                    @*<input type="text" class="w-full" placeholder="i.e. www.clubglobal.com" name="WebsiteLink" value="@Model.WebsiteLink" ="">*@
                                    <span class="error">
                                        @Html.ValidationMessageFor(m => m.GoogleMap)
                                    </span>
                                    @*<input type="text" class="w-full" placeholder="i.e. www.googlemap.com/" name="googleMap" required>
                                   <span class="error">Validation msg</span>*@
