                            <div class="input-field-common">
                                <label class="block mb-[12px] text-sm font-medium text-secondary-text-2 ">
                                    @CRS.ADMIN.APPLICATION.Resources.Resource.Club
                                </label>
                                <select id="large" name="Club" class=" default_select select-common block w-full "
                                        style="width: 100%;">
                                    <option>Select Club Type</option>
                                    <option value="CA">club</option>
                                    <option value="FR">club</option>
                                    <option value="DE">club</option>
                                </select>
                            </div>
                            <div class="input-field-common">
